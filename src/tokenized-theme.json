  "tokenColors": [
    {
      "scope": [
        "comment"
      ],
      "settings": {
        "foreground": "$clay",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant"
      ],
      "settings": {
        "foreground": "$leaf"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.language"
      ],
      "settings": {
        "foreground": "$sakura"
      }
    },
    {
      "scope": [
        "entity.name"
      ],
      "settings": {
        "foreground": "$sakura"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "$iris"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "$wisteria",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "invalid"
      ],
      "settings": {
        "foreground": "$lily"
      }
    },
    {
      "scope": [
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "$dust"
      }
    },
    {
      "scope": [
        "keyword",
        "variable.language.this"
      ],
      "settings": {
        "foreground": "$leaf"
      }
    },
    {
      "scope": [
        "markup.inserted.diff"
      ],
      "settings": {
        "foreground": "$iris"
      }
    },
    {
      "scope": [
        "markup.deleted.diff"
      ],
      "settings": {
        "foreground": "$lily"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.bold.markdown",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic.markdown",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "meta.diff.range"
      ],
      "settings": {
        "foreground": "$wisteria"
      }
    },
    {
      "scope": [
        "meta.tag",
        "meta.brace"
      ],
      "settings": {
        "foreground": "$powder"
      }
    },
    {
      "scope": [
        "meta.import",
        "meta.export"
      ],
      "settings": {
        "foreground": "$leaf"
      }
    },
    {
      "scope": "meta.directive.vue",
      "settings": {
        "foreground": "$wisteria",
        "fontStyle": "italic"
      }
    },
    {
